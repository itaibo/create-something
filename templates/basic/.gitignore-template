/node_modules
/build
/dist
/.vscode
/.idea
/bun.lockb

# Ignore log and temporary files
*.log
*.tmp

# Ignore environment-specific files
.env
.env.local

# Ignore compiled binaries and libraries
*.pyc
*.class
*.jar

# Ignore system and hidden files
.DS_Store
Thumbs.db
